<h1>PARSEL</h1>

<form action='/parcel'>
  <div class="form-group">
    <label for="length">Length in inches</label>
    <input id="length" name="length" class="form-control" type="text" placeholder="ex: 5.5" autofocus>
  </div>
  <div class="form-group">
    <label for="width">Width in inches</label>
    <input id="width" name="width" class="form-control" type="text" placeholder="ex: 6">
  </div>
  <div class="form-group">
    <label for="height">Height in inches</label>
    <input id="height" name="height" class="form-control" type="text" placeholder="ex: 3">
  </div>
<div class="form-group">
  <label for="weight">Weight in pounds</label>
  <input id="weight" name="weight" class="form-control" type="text" placeholder="ex: 8">
</div>
  <button type="submit" class="btn">Calculate Shipping</button>
</form>
